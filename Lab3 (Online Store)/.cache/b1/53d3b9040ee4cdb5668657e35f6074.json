        }\r\n\r\n        a {\r\n            padding: 8px;\r\n            background-color: rgb(167, 255, 5);\r\n            text-decoration: dashed;\r\n            border-radius: 8px;\r\n        }\r\n\r\n        .cart box-icon{\r\n            \r\n            vertical-align: bottom;\r\n            font-size: 22px;\r\n            padding-right: 5px;\r\n\r\n            a {\r\n            background-color: bisque;\r\n            color: tomato ;\r\n            cursor: pointer;\r\n            text-decoration: none;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n\r\n// specific classes\r\n\r\n.cover {\r\n    position: absolute;\r\n    z-index: 1;\r\n    top: 0;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: 0;\r\n    height: 100%;\r\n    padding: 0 !important;\r\n    margin: 0 !important;\r\n    background-color: rgba(152, 251, 152, 0.2)\r\n}\r\n\r\n\r\n.container, \r\n.products-container{\r\n    display: flex;\r\n    justify-content: center;\r\n    flex-wrap: wrap;\r\n    margin-top: 60px;\r\n    padding-bottom: 6rem;\r\n    \r\n    .add_cart {\r\n        position: absolute;\r\n        width: 100%;\r\n        background-color: lightgreen;\r\n        transition: all 0.35s ease-in-out;\r\n        opacity: 0;\r\n        cursor: pointer;\r\n        text-align: center;\r\n    }\r\n\r\n    .image {\r\n        margin-right: 5px 20px 0 20px;\r\n        position: relative;\r\n        overflow: hidden;\r\n        \r\n        a {\r\n            padding: 2px;\r\n            background-color: $color50;\r\n            text-decoration: dotted;\r\n        }\r\n\r\n        &:hover .cart1, &:hover .cart2, &:hover .cart3, &:hover .cart4 {\r\n                bottom: 58px;\r\n                opacity: 1;\r\n                padding: 10px;\r\n                text-decoration: none;\r\n            }\r\n    }\r\n\r\n}\r\n\r\n// __________________The Card Side\r\n\r\n.products-container {\r\n    max-width: 720px;\r\n    justify-content: space-around;\r\n    margin: 0 auto;\r\n    margin-top: 40px;\r\n\r\n    #remove{\r\n        margin-left: 0px;\r\n        margin-right: 10px;\r\n        cursor: pointer;\r\n    }\r\n    \r\n    \r\n    .product-header {\r\n        width: 100%;\r\n        max-width: 720px;\r\n        display: flex;\r\n        justify-content: flex-start;\r\n        border-bottom: 4px solid $color50;\r\n        margin: 0 auto;\r\n\r\n        .product-title {\r\n            width: 40%;\r\n        }\r\n\r\n        .price {\r\n            width: 15%;\r\n            display: flex;\r\n            align-items: center;\r\n            border-bottom: 4px solid rgb(33, 197, 12);\r\n        }\r\n        // .size {\r\n        //     width: 20%;\r\n        //     display: flex;\r\n        //     align-items: center;\r\n        // }\r\n\r\n        .quantity {\r\n            width: 20%;\r\n            border-bottom: 4px solid $color50;\r\n            display: flex;\r\n            align-items: center;\r\n        }\r\n\r\n        .total {\r\n            width: 25%;\r\n            border-bottom: 1px, solid $color50;\r\n            display: flex;\r\n            align-items: center;\r\n            border-bottom: 8px dotted darkslateblue;\r\n        }\r\n    }\r\n}\r\n\r\n\r\n.product {\r\n    width: 40%;\r\n    display: flex;\r\n    justify-content: space-around;\r\n    align-items: center;\r\n    padding: 10px 0;\r\n    border-bottom: 4px solid $color50;\r\n    \r\n    .price{\r\n        align-items: center;\r\n        justify-content: center;\r\n    }\r\n}\r\n\r\n.products {\r\n    width: 100%;\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n\r\n    .product img {\r\n        width: 110px;\r\n        height: 75px;\r\n        // margin-right: 25px;\r\n    }\r\n    .price {\r\n        width: 15%;\r\n        border-bottom: 4px solid $color50;\r\n        align-items: center;\r\n        justify-content: center;\r\n    }\r\n    .quantity {\r\n        width: 20%;\r\n        border-bottom: 4px solid $color50;\r\n        #quantity-inc{\r\n            cursor: pointer;\r\n        }\r\n        #quantity-dec{\r\n            cursor: pointer;\r\n        }\r\n    }\r\n    .total {\r\n        border-bottom: 4px solid $color50;\r\n    }\r\n}\r\n\r\n.basketTotalContainer {\r\n    background-color: $color50;\r\n    display: flex;\r\n    justify-content: flex-end;\r\n    width: 100%;\r\n    padding: 10px 0;\r\n    margin-top: 40px;\r\n\r\n    .basketTitle {\r\n        width: 30%;\r\n    }\r\n    .basketTotalArrows{\r\n        width: 22%;\r\n    }\r\n\r\n    .basketTotal {\r\n        width: 15%;\r\n    }\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"6f500d32421f9e8c2c1bfbaa3068e469","cacheData":{"env":{}}}